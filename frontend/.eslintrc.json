{
    "extends": "airbnb",
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint", "react-hooks"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "useJSXTextNode": true
    },
    "rules": {
        "quotes": ["error", "single"],
        "react-hooks/rules-of-hooks": "error"
    }
}